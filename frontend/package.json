{
  "name": "twettrybust",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm-run-all stylelint-build vite-build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "stylelint-dev": "stylelint 'src/**/*.scss' --cache --cache-location './.stylelintcache'; exit 0",
    "stylelint-build": "stylelint 'src/**/*.scss'",
    "watch-styles-sass": "npm-watch",
    "start-js": "vite",
    "start": "npm-run-all -p watch-styles-sass start-js",
    "stylelint-fix-sass": "stylelint --fix 'src/**/*.scss'",
    "vite-build": "vite build"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "firebase": "^10.7.1",
    "formik": "^2.4.5",
    "i18next": "^23.7.16",
    "i18next-browser-languagedetector": "^7.2.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.0",
    "react-icons": "^5.0.1",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.2",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^3.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "classnames": "^2.5.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "npm-run-all": "^4.1.1",
    "npm-watch": "^0.3.0",
    "sass": "^1.69.7",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-order": "^0.7.0",
    "vite": "^5.0.8"
  },
  "watch": {
    "stylelint-dev": {
      "patterns": [
        "src/**/*.scss"
      ],
      "extensions": "scss"
    }
  }
}
